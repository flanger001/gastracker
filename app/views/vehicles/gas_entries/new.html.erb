<% content_for :title, "Add Fill-Up" %>
<%= form_with model: [vehicle, resource] do |form| %>
  <%= render "errors" %>
  <%= render "fields", form: form %>

  <br/>
  <%= form.submit "Add Gas Entry", class: "btn btn-success btn-lg" %>
  <%= link_to "Back", :back, class: "btn btn-default btn-lg" %>
<% end %>
